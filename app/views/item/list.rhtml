<h1>Listing TravellerBooks</h1>

<table>
  <tr>
  <% for column in Item.content_columns %>
    <th><%= column.human_name %></th>
  <% end %>
  </tr>
  
<% for item in @items %>
  <tr>
  <% for column in Item.content_columns %>
    <td><%=h item.send(column.name) %></td>
  <% end %>
    <td><%= link_to 'Show', :action => 'show', :id => item.tbid %></td>
  </tr>
<% end %>
</table>

<br />
